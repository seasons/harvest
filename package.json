{
  "scripts": {
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios": "react-native run-ios",
    "start": "node ./src/schemaQuery.ts && yarn start-servers",
    "start-servers": "concurrently --kill-others 'yarn start-packager'",
    "test": "jest",
    "jetify": "jetify",
    "web": "expo start --web",
    "start-packager": "env REACT_DEBUGGER=\"$([ -d '/Applications/React Native Debugger.app' ] && echo \"open 'rndebugger://set-debugger-loc?host=localhost&port=8081' --args\")\" node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js start",
    "fix-androidx": "bash ./scripts/fix-androidx.sh",
    "postinstall": "bash ./scripts/fix-androidx.sh",
    "apollo": "apollo service:push && rm -rf generated && apollo codegen:generate --excludes=node_modules/* --target typescript --tagName gql --outputFlat src/generated"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@expo/react-native-fade-in-image": "^1.1.1",
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/datetimepicker": "^2.4.2",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/netinfo": "^5.7.0",
    "@react-native-community/picker": "^1.6.3",
    "@react-navigation/bottom-tabs": "^5.0.5",
    "@react-navigation/native": "^5.0.5",
    "@react-navigation/stack": "^5.0.8",
    "@segment/analytics-react-native": "^1.1.0",
    "@segment/analytics-react-native-google-analytics": "^1.1.0",
    "@segment/analytics-react-native-mixpanel": "^1.1.0",
    "@sentry/react-native": "^1.1.0",
    "@types/jest": "^24.0.18",
    "apollo-boost": "^0.4.4",
    "apollo-cache": "^1.3.2",
    "apollo-client": "2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-utilities": "^1.3.2",
    "aws-sdk": "^2.639.0",
    "expo": "^35.0.0",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "lottie-ios": "3.1.3",
    "lottie-react-native": "~3.3.2",
    "luxon": "^1.21.1",
    "node-fetch": "^2.6.0",
    "querystring": "^0.2.0",
    "react": "16.9.0",
    "react-apollo": "^3.1.1",
    "react-dom": "16.9.0",
    "react-native": "0.61.5",
    "react-native-animatable": "^1.3.3",
    "react-native-auth0": "^2.1.0",
    "react-native-config": "^0.12.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-permissions": "^2.0.9",
    "react-native-pusher-push-notifications": "^2.4.0",
    "react-native-reanimated": "^1.8.0",
    "react-native-restart": "^0.0.14",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-share": "^3.3.0",
    "react-native-sortable-list": "^0.0.23",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "9.13.3",
    "react-native-unimodules": "0.6.0",
    "react-native-web": "^0.11.7",
    "react-native-webview": "7.4.3",
    "react-spring": "^8.0.27",
    "react-tracking": "^7.3.0",
    "reanimated-bottom-sheet": "^1.0.0-alpha.20",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1",
    "tipsi-stripe": "^7.5.1",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.7.2",
    "@types/node": "^13.9.3",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.61.5",
    "@types/react-tracking": "^7.0.2",
    "@types/styled-system": "^5.1.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-expo": "^7.0.0",
    "concurrently": "2.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.1.0",
    "jest-raw-loader": "1.0.1",
    "jest-styled-components": "7.0.0-2",
    "jetifier": "^1.6.4",
    "react-test-renderer": "^16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "remote-redux-devtools": "^0.5.16",
    "snapshot-diff": "0.2.2",
    "typescript": "^3.7.0",
    "waait": "^1.0.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^App/(.*)$": "<rootDir>/src/$1",
      "^Assets/(.*)$": "<rootDir>/assets/$1",
      "^Components/(.*)$": "<rootDir>/src/Components/$1",
      "^Scenes/(.*)$": "<rootDir>/src/Scenes/$1",
      "^Utils/(.*)$": "<rootDir>/src/utils/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/*-tests.(ts|tsx|js)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "./src/setupJest.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "cacheDirectory": ".jest/cache",
    "transform": {
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|mp4|png|psd|svg|webp)$": "<rootDir>/node_modules/react-native/jest/assetFileTransformer.js",
      ".*(ts|tsx|js|jsx)$": "babel-jest",
      "\\.graphql$": "jest-raw-loader",
      "node_modules/react-native/.+\\.(js|tsx?)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/aws-sdk/dist/aws-sdk-react-native.js",
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "private": true
}
