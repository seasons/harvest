{
  "name": "harvest",
  "version": "1.10.6",
  "engines": {
    "node": ">=12.x.x"
  },
  "scripts": {
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios": "react-native run-ios",
    "start": "node ./src/schemaQuery.ts && yarn start-servers",
    "start-servers": "concurrently --kill-others 'yarn start-packager'",
    "test": "jest",
    "jetify": "jetify",
    "web": "expo start --web",
    "start-packager": "env REACT_DEBUGGER=\"$([ -d '/Applications/React Native Debugger.app' ] && echo \"open 'rndebugger://set-debugger-loc?host=localhost&port=8081' --args\")\" node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js start",
    "fix-androidx": "bash ./scripts/fix-androidx.sh",
    "postinstall": "bash ./scripts/fix-androidx.sh",
    "apollo": "apollo service:push && rm -rf generated && apollo codegen:generate --excludes=node_modules/* --target typescript --tagName gql --outputFlat src/generated",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postversion": "yarn build:fastlane",
    "build:fastlane": "fastlane ios beta"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@material-ui/core": "^4.11.1",
    "@react-native-community/async-storage": "~1.12.0",
    "@react-native-community/clipboard": "^1.5.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.6",
    "@react-native-community/picker": "1.6.6",
    "@react-navigation/bottom-tabs": "^5.7.2",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "@seasons/eclipse": "^1.1.4",
    "@segment/analytics-react-native": "^1.1.0",
    "@segment/analytics-react-native-google-analytics": "^1.1.0",
    "@segment/analytics-react-native-mixpanel": "^1.1.0",
    "@sentry/react-native": "^1.1.0",
    "@types/jest": "^24.0.18",
    "@types/luxon": "^1.24.3",
    "apollo-boost": "^0.4.4",
    "apollo-cache": "^1.3.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-upload-client": "^14.1.1",
    "apollo-utilities": "^1.3.2",
    "aws-sdk": "^2.639.0",
    "expo": "^39.0.0",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "luxon": "^1.21.1",
    "node-fetch": "^2.6.0",
    "querystring": "^0.2.0",
    "react": "16.13.1",
    "react-apollo": "^3.1.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.3",
    "react-native-animatable": "^1.3.3",
    "react-native-auth0": "^2.1.0",
    "react-native-config": "^0.12.0",
    "react-native-contacts": "^6.0.3",
    "react-native-device-info": "^5.6.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-fade-in-image": "^1.5.0",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-image-picker": "^2.3.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.2",
    "react-native-permissions": "^2.0.9",
    "react-native-pusher-push-notifications": "^2.4.0",
    "react-native-rate": "^1.2.1",
    "react-native-reanimated": "~1.13.0",
    "react-native-restart": "^0.0.14",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-scroll-bottom-sheet": "^0.7.0",
    "react-native-share": "4.0.2",
    "react-native-shared-element": "0.7.0",
    "react-native-sortable-list": "^0.0.23",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.13.7",
    "react-native-webview": "10.7.0",
    "react-navigation": "^4.4.0",
    "react-navigation-shared-element": "^3.0.0",
    "react-navigation-stack": "^2.8.2",
    "react-primitives": "^0.8.1",
    "react-spring": "^8.0.27",
    "react-tracking": "^7.3.0",
    "reanimated-bottom-sheet": "^1.0.0-alpha.20",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1",
    "tipsi-stripe": "https://github.com/ashfurrow/tipsi-stripe.git#fix-infinite-loop",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.7.2",
    "@react-native-community/cli": "^4.13.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-ondevice-actions": "^5.3.21",
    "@storybook/react-native": "^5.3.21",
    "@storybook/react-native-server": "^5.3.21",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/node": "^13.9.3",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-tracking": "^7.0.2",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-expo": "^8.3.0",
    "concurrently": "2.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.1.0",
    "jest-raw-loader": "1.0.1",
    "jest-styled-components": "7.0.0-2",
    "jetifier": "^1.6.4",
    "react-is": "^16.13.1",
    "react-native-unimodules": "^0.11.0",
    "react-test-renderer": "^16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "remote-redux-devtools": "^0.5.16",
    "snapshot-diff": "0.2.2",
    "typescript": "~3.9.2",
    "waait": "^1.0.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^App/(.*)$": "<rootDir>/src/$1",
      "^Assets/(.*)$": "<rootDir>/assets/$1",
      "^Components/(.*)$": "<rootDir>/src/Components/$1",
      "^Scenes/(.*)$": "<rootDir>/src/Scenes/$1",
      "^Utils/(.*)$": "<rootDir>/src/utils/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/*-tests.(ts|tsx|js)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "./src/setupJest.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "cacheDirectory": ".jest/cache",
    "transform": {
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|mp4|png|psd|svg|webp)$": "<rootDir>/node_modules/react-native/jest/assetFileTransformer.js",
      ".*(ts|tsx|js|jsx)$": "babel-jest",
      "\\.graphql$": "jest-raw-loader",
      "node_modules/react-native/.+\\.(js|tsx?)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/aws-sdk/dist/aws-sdk-react-native.js",
      "node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "private": true
}
