{
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start && yarn start-packager",
    "test": "jest",
    "web": "expo start --web",
    "start-packager": "env REACT_DEBUGGER=\"$([ -d '/Applications/React Native Debugger.app' ] && echo \"open 'rndebugger://set-debugger-loc?host=localhost&port=8081' --args\")\" node node_modules/react-native/local-cli/cli.js start"
  },
  "dependencies": {
    "expo": "^34.0.1",
    "lodash": "^4.17.15",
    "native-base": "^2.13.5",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "0.59.10",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-navigation": "3.1.1",
    "react-native-reanimated": "~1.1.0",
    "react-native-svg": "^9.9.3",
    "react-native-unimodules": "~0.5.2",
    "react-native-web": "^0.11.4",
    "react-spring": "^8.0.27",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.57.65",
    "@types/styled-system": "^5.1.1",
    "babel-preset-expo": "^6.0.0",
    "jest-expo": "^34.0.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "private": true
}
